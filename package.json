  "version": "0.0.0",
  "type": "module",
  "main": "./main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron ./main.js --no-sandbox",
