    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "mysql2": "^3.14.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "sequelize": "^6.37.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
